pipeline {
    agent any
    stages {
        stage(" execute sql script") {
           steps {
					ansiblePlaybook(
					installation: 'Ansible',
                    inventory: 'inventory.ini',
					playbook: 'execute_sql_script.yml',
                    credentialsId: 'ansibleUser',
                    extraVars   : [
                        c_sid: env.c_sid,
                        c_blocking: env.c_blocking,
                        sql_id: env.sql_id,
                    ]
                )
            }    
        }    
    }
}
